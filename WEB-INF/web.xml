<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<!-- web-app element was originally found empty - the following are additions 
      for the demonstration of HTTPGetServlet -->
<web-app>
  
  <servlet> <!-- the following name will refer to the sepecified class -->
     <servlet-name>formA</servlet-name>
     <servlet-class>formA</servlet-class>
  </servlet> 
	<servlet>
		<servlet-name>formB</servlet-name>
		<servlet-class>formB</servlet-class>
	</servlet>    
	<servlet>
		<servlet-name>formC</servlet-name>
		<servlet-class>formC</servlet-class>
	</servlet>    

   <servlet-mapping>
     <servlet-name>formA</servlet-name>
     <url-pattern>/formA/*</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
     <servlet-name>formB</servlet-name>
     <url-pattern>/formB/*</url-pattern>
   </servlet-mapping>   
   <servlet-mapping>
     <servlet-name>formC</servlet-name>
     <url-pattern>/formC/*</url-pattern>
   </servlet-mapping>   
   
</web-app>
